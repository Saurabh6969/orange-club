<configuration>
  <appSettings>
    <!--<add key="sqlConString" value="Data Source=10.1.67.25;Initial Catalog=OrangeDb;Integrated Security=True"/>-->
    <!--<add key="sqlConString" value="Data Source=.\SQLEXPRESS;AttachDbFilename=OrangeDb.mdf;Integrated Security=True;User Instance=True"/>-->
    <add key="sqlConString" value="Data Source=.\SQLEXPRESS;Initial Catalog=OrangeDb;Integrated Security=True;Integrated Security=True"/>
    <add key="selectLogin" value="select tbl_Login.userName,passWord,role,memberId from tbl_Login,tbl_member where tbl_member.username=tbl_Login.username and (tbl_Login.username=@user and passWord=@pass) and tbl_member.accountStatus=1"/>
    <add key="selectMemberInfo" value="SELECT memberId,fName,lName,address,email,phone,dob,doj,memberType,accountStatus,statusRemark,maritalStatus,sportsPerson,deptId,username,dateOfExpiry,gender,due,memberID FROM tbl_member where memberId=@memberId"/>
    <add key="MemberInfoFromUsername" value="select memberId from tbl_member where username=@username"/>
    <add key="lockAccount" value="update tbl_member set tbl_member.accountStatus=3 where username=@user"/>
    <add key="insertMember" value="insertMemberDetails"/>
    <add key="selectNewMember" value="SELECT memberId,fName,lName,address,email,phone,dob,doj,memberType,accountStatus,statusRemark,maritalStatus,sportsPerson,deptId,username,dateOfExpiry,gender FROM tbl_member accountStatus=0"/>
    <add key="selectNewMember" value="SELECT memberId,fName,lName,address,email,phone,dob,doj,membertype,accountStatus,statusRemark,maritalStatus,sportsPerson,deptId,username,dateOfExpiry,gender FROM tbl_member where accountStatus=0"/>
    <add key="selectBillInfo" value="SELECT billId,memberId,paymentMode,paymentFor,Date,due,amountPaid,chequeNo,branchName,bankName,cardNo,cardType FROM tbl_billingInfo WHERE memberId=1"/>
    <add key="addAddOn" value="INSERT INTO tbl_Addon (memberId,referenceId,relation) VALUES (@memberId,@referenceId,@relation)"/>
    <add key="updateMember" value="updateMemberDetails"/>
    <add key="Renewalmembership" value="SELECT memberId,fName,doj,memberType,dateOfExpiry from tbl_member where memberid=1"/>
    <add key="acceptMember" value="Update tbl_member set accountStatus=1 where memberId=@m"/>
    <add key="rejectMember" value="Update tbl_member set accountStatus=2 where memberId=@m"/>
    <!--START:added by shrutee-->
    <add key="selectBillStatus" value="select sum(due) from tbl_member where memberId=@memberId"/>
    <add key="selectDates" value="select doj,dateOfExpiry,memberType from tbl_member where memberId=@memberId"/>
    <add key="selectRef" value="select memberid,doj from tbl_member m where memberid=(select referenceid from tbl_addon where m.memberid=@memberid)"/>
    <add key="selectUpgradFee" value="select temporaryMemFee from tbl_generalFee where feeId=3"/>
    <!--END:added by shrutee-->
    <!--meenaxi -->
    <add key="permanentRegistrationFee" value="select sum(permanentMemFee) from tbl_GeneralFee where feeID in (1,2,4)"/>
    <add key="temporaryRegistrationFee" value="select sum(temporaryMemFee) from tbl_GeneralFee where feeID in (1,2,4)"/>
    <add key="addonRegistrationFee" value="select sum(addonMemFee) from tbl_GeneralFee where feeID in (1,2,4)"/>
    <!--END-->
    <!--    <add key ="viewBill" value="SELECT billId, memberId, paymentMode, paymentFor, Date, amountPaid, paymentRemark FROM tbl_billingInfo WHERE ((memberId = @memberId) AND (Date &lt;= @Date1) AND (Date &gt;= @Date2)) ORDER BY Date" />
  -->
    <add key="viewBill" value="SELECT billId, memberId, paymentMode, paymentFor, Date, amountPaid, paymentRemark FROM tbl_billingInfo WHERE (memberId = @memberId) AND Date between @Date1 AND @Date2 ORDER BY Date"/>
    <!-- Changes by client on 2011-9-28-->
    <add key="totalMember" value="Select count(memberId) from tbl_member "/>
    <add key="selectedMember" value="Select count(memberId) from tbl_member where accountStatus=@status and memberType=@memberType"/>
    <add key="getRevenue" value="select sum(amountPaid) from tbl_billingInfo where memberId in (select memberId from tbl_member where memberType=@memberType)"/>
    <add key="countDefaulter" value="select count(memberId) from tbl_member where due &lt;&gt; 0"/>
    <!-- end of changes-->
  </appSettings>
  <connectionStrings>
    <add name="OrangeDbConnectionString" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=OrangeDb;Integrated Security=True;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="OrangeDbConnectionString2" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=OrangeDb;Integrated Security=True;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="OrangeDbConnectionString5" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=OrangeDb;Integrated Security=True;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="OrangeDbConnectionString3" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=OrangeDb;Integrated Security=True;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
